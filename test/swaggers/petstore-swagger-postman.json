                  "  var jsonData = pm.response.json();",
                  "  pm.expect(Array.isArray(jsonData)).to.eql(true);",
                  "});",
                  "// Test whether the response matches the schema",
                  "const responseSchema = {",
                  "  \"type\": \"array\",",
                  "  \"items\": {",
                  "    \"type\": \"object\",",
                  "    \"required\": [\"name\", \"photoUrls\"],",
                  "    \"properties\": {",
                  "      \"id\": {",
                  "        \"type\": \"integer\",",
                  "        \"format\": \"int64\"",
                  "      },",
                  "      \"category\": {",
                  "        \"type\": \"object\",",
                  "        \"properties\": {",
                  "          \"id\": {",
                  "            \"type\": \"integer\",",
                  "            \"format\": \"int64\"",
                  "          },",
                  "          \"name\": {",
                  "            \"type\": \"string\"",
                  "          }",
                  "        },",
                  "        \"xml\": {",
                  "          \"name\": \"Category\"",
                  "        }",
                  "      },",
                  "      \"name\": {",
                  "        \"type\": \"string\",",
                  "        \"example\": \"doggie\"",
                  "      },",
                  "      \"photoUrls\": {",
                  "        \"type\": \"array\",",
                  "        \"xml\": {",
                  "          \"name\": \"photoUrl\",",
                  "          \"wrapped\": true",
                  "        },",
                  "        \"items\": {",
                  "          \"type\": \"string\"",
                  "        }",
                  "      },",
                  "      \"tags\": {",
                  "        \"type\": \"array\",",
                  "        \"xml\": {",
                  "          \"name\": \"tag\",",
                  "          \"wrapped\": true",
                  "        },",
                  "        \"items\": {",
                  "          \"type\": \"object\",",
                  "          \"properties\": {",
                  "            \"id\": {",
                  "              \"type\": \"integer\",",
                  "              \"format\": \"int64\"",
                  "            },",
                  "            \"name\": {",
                  "              \"type\": \"string\"",
                  "            }",
                  "          },",
                  "          \"xml\": {",
                  "            \"name\": \"Tag\"",
                  "          }",
                  "        }",
                  "      },",
                  "      \"status\": {",
                  "        \"type\": \"string\",",
                  "        \"description\": \"pet status in the store\",",
                  "        \"enum\": [\"available\", \"pending\", \"sold\"]",
                  "      }",
                  "    },",
                  "    \"xml\": {",
                  "      \"name\": \"Pet\"",
                  "    }",
                  "  }",
                  "};",
                  "",
                  "var response = JSON.parse(responseBody);",
                  "tests[\"Schema is valid\"] = tv4.validate(response, responseSchema);"
                ]
